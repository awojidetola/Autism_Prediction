    "## Modelling"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv(\"Training_Data.csv\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = df.drop(['Unnamed: 0'],1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "qualitative = df[['gender','jundice','austim','Age Group']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "nominal_data = pd.get_dummies(qualitative)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = df.drop(['gender','jundice','austim','Age Group'],1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "data = pd.concat([nominal_data,df],1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "X = data.drop(['Class/ASD'],1)\n",
    "y = data[['Class/ASD']]\n",
    "y = y.replace({'NO':0,'YES':1})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "X_train,X_test,y_train,y_test = train_test_split(X,y,test_size=0.25,random_state=42)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "dftest = pd.read_csv(\"Testing Features.csv\")\n",
    "\n",
    "dftest = dftest.drop(['Unnamed: 0'],1)\n",
    "\n",
    "qualitative = dftest[['gender','jundice','austim','Age Group']]\n",
    "\n",
    "nominal_data = pd.get_dummies(qualitative)\n",
    "\n",
    "dftest = dftest.drop(['gender','jundice','austim','Age Group'],1)\n",
    "\n",
    "datatest = pd.concat([nominal_data,dftest],1)\n",
    "\n",
    "true_values = pd.read_csv(\"Actual Values.csv\")\n",
    "true_values = true_values.drop(['Unnamed: 0'],1)\n",
    "true_values = true_values.replace({\"NO\":0,\"YES\":1})"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
